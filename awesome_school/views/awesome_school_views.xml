<?xml version="1.0"encodeing="utf-8" ?>
<odoo>
    <data noupdate="1">
    <record id="awesome_school_form" model="ir.ui.view">
        <field name="name">awesome.school.form</field>
            <field name="res_model">awesome.school</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <group>
                                <field name="name" />
                                <field name="first" />
                                <field name="last" />
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>

    </record>
        <record id="awesome_school_tree" model="ir.ui.view">
        <field name="name">awesome.school.tree</field>
            <field name="res_model">awesome.school</field>
            <field name="arch" type="xml">
                <tree string="Schools">
                                <field name="name" />
                                <field name="first" />
                                <field name="last" />
                </tree>
            </field>

    </record>
        <record id="awesome_school_action" model="ir.actions.act_window">
            <field name="name">Schools</field>
            <field name="res_model">awesome.school</field>

        </record>
        <menuitem id="awesome_school_root" name="Awesome School" action="awesome_school_action" />
    </data>

</odoo>